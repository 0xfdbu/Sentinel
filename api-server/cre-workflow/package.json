{
  "name": "cre-contract-security-scanner",
  "version": "1.0.0",
  "description": "Chainlink CRE workflow for smart contract security analysis using Etherscan and XAI Grok",
  "main": "contract-scanner.ts",
  "scripts": {
    "simulate": "cre workflow simulate . --target devnet-settings",
    "simulate:interactive": "cre workflow simulate . --target devnet-settings",
    "simulate:sepolia": "cre workflow simulate . --target staging-settings",
    "deploy": "cre workflow deploy . --target production-settings",
    "deploy:staging": "cre workflow deploy . --target staging-settings",
    "activate": "cre workflow activate . --target production-settings",
    "pause": "cre workflow pause . --target production-settings",
    "delete": "cre workflow delete . --target production-settings"
  },
  "keywords": [
    "chainlink",
    "cre",
    "smart-contract",
    "security",
    "audit",
    "etherscan",
    "xai",
    "grok"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@chainlink/cre-sdk": "^1.0.9",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}
